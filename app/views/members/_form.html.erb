<%= form_for member do |f| %>
  <%= render partial: "shared/error_messages", locals: {object: f.object} %>
  <div class="item-form-wrapper">
    <%= f.label :name, "名前", class:"form-label" %>
    <%= f.text_field :name %>
    <%= f.label :category, "カテゴリ", class:"form-label" %>
    <%= f.text_field :category %>
    <%= f.label :birthday, "誕生日", class:"form-label" %>
    <%= f.date_field :birthday %>
    <%= f.submit yield(:submit) %>
  <% end %>
</div>
